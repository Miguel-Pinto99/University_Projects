<launch>

    <rosparam command="load" file="$(find p_fyou_bringup)/params/game.yaml"/>

    <!--RED TEAM-->
    <include file="$(find p_fyou_bringup)/launch/spawn.launch">
        <arg name="x_pos" value="5.0"/>
        <arg name="y_pos" value="0.0"/>
        <arg name="z_pos" value="0.0"/>
        <arg name="player_name" value="red1"/>
