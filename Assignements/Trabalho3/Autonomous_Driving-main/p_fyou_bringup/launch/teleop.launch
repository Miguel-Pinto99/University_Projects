<launch>
   <arg name="player_name" default="p_fyou" />
    <group ns="$(arg player_name)"> <!-- /differential_drive_controller/ -->
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
    <param name="model" value="$(arg model)"/>

  <!-- turtlebot3_teleop_key already has its own built in velocity smoother -->
  <node pkg="p_fyou_bringup" type="turtlebot3_teleop" name="$(arg player_name)_turtlebot3_teleop_keyboard"  output="screen" ns="differential_drive_controller">
  </node>
      </group>
</launch>

<!--qwe-->


